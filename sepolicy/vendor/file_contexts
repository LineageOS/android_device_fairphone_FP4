# Audio
/mnt/vendor/persist/factory/audio/aw_cali\.bin                             u:object_r:vendor_persist_audio_file:s0

# Camera
/dev/stmvl53l1_ranging                                                     u:object_r:vl53l1_device:s0
/sys/class/board_id/version                                                u:object_r:tct_mmitest_sysfs:s0

# Fingerprint
/dev/fpsensor                                                              u:object_r:fingerprint_device:s0

# MAC (Bluetooth)
/mnt/vendor/persist/t2m_param(/.*)?                                        u:object_r:persist_t2m_param_file:s0

# NFC
/dev/st21nfc                                                               u:object_r:nfc_device:s0
/vendor/bin/hw/android\.hardware\.nfc@1\.2-service-st                      u:object_r:hal_nfc_default_exec:s0

# SSR
/sys/devices/platform/soc/aab0000\.qcom,venus/subsys[0-9]+/name            u:object_r:vendor_sysfs_ssr:s0
/sys/devices/platform/soc/aab0000\.qcom,venus/subsys[0-9]+/restart_level   u:object_r:vendor_sysfs_ssr_toggle:s0

# Partitions
/dev/block/platform/soc/1d84000\.ufshc/by-name/traceability                u:object_r:traceability_block_device:s0
/dev/block/platform/soc/1d84000\.ufshc/by-name/tunning                     u:object_r:vendor_modem_efs_partition_device:s0

# Tct daemon
/vendor/bin/tctd                                                           u:object_r:tctd_exec:s0
